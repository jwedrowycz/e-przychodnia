{% extends 'admin_panel/index.html.twig' %}


{% block content %}
    {% for label, messages in app.flashes(['success', 'fail']) %}
        {% for message in messages %}
            <div class="flash-{{ label }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
    <h1>{{ user.imie ~ ' ' ~ user.nazwisko }}</h1>
    {% if user.roles[0] == "ROLE_USER" %}
        <p>UÅ¼ytkownik</p>
    {% elseif user.roles[0] == "ROLE_ADMIN" %}
        <p>Administrator</p>
    {% else %}
        <p>Operator</p>
    {% endif %}
     <p>{{ user.id }}</p>
    <p>{{ user.email }}</p>
    <p>{{ user.imie ~ ' ' ~ user.nazwisko}}</p>
    <p>{{ user.PESEL }}</p>
    <p>{{ user.telefon }}</p>
    <p>{{ user.adresZamieszkania }}</p>
    <p>{{ user.miasto ~ ' ' ~ user.kodPocztowy }}</p>
    <p>{{ user.wojewodztwo }}</p>
    <p>{{ user.dataUrodzenia|date("d/m/Y") }}</p>
    <p>{{ user.plec }}</p>
    <p>{{ user.roles[0] }}</p>
{% endblock %}