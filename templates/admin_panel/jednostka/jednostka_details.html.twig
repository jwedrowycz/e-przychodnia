{% extends 'admin_panel/index.html.twig' %}

{% block content %}
    {% for label, messages in app.flashes(['success', 'fail']) %}
    {% for message in messages %}
        <div class="flash-{{ label }}">
            {{ message }}
        </div>
    {% endfor %}
{% endfor %}
{% for j in jednostka %}
     <h2>Lekarz: {{ j.nazwisko ~ ' ' ~ j.imie}}</h2>
     <h2>{{ j.nazwa }} </h2>
     {% endfor %}
     <h3>Godziny pracy poradni</h3>
    <table class="admin-table">
        <tr>
            <th>Dzie≈Ñ tygodnia</th>
            <th>Od</th>
            <th>Do</th>
        </tr>
    {% for c in czas %}
        <tr>
            <td>{{ c.dzien }}</td>
            <td>{{ c.start|date("H:i") }}</td>
            <td>{{ c.koniec|date("H:i") }}</td>
        </tr>
    {% endfor %}
    </table>

  
{% endblock content %}
