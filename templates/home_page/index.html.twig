{% extends 'base.html.twig' %}

{% block title %}Przychodnia rejonowa{% endblock %}

{% block body %}

    <div class="container">
        {% for label, messages in app.flashes(['success', 'fail']) %}
            {% for message in messages %}
                <div class="flash-{{ label }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
        <h1>Strona główna przychodni</h1>
       
    </div>

{% endblock %}
